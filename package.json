{
  "name": "coronavirus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-md": "^1.15.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-vertical-timeline-component": "^2.5.0"
  },
  "scripts": {
    "start": "npm run copySW && react-app-rewired start",
    "build": "react-app-rewired build && npm run copySW",
    "copySW": "workbox copyLibraries public/third_party/workbox/",
    "test": "react-app-rewired test --env=jsdom",
    "serve": "serve -s build",
    "eject": "react-scripts eject",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "start-storybook -p 9009 -s public,static,build",
    "build-storybook": "build-storybook -s public -o stories"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.12",
    "node-sass": "^4.13.1",
    "react-app-rewire-workbox": "^2.0.1",
    "react-app-rewired": "^2.1.5",
    "react-styleguidist": "^10.6.1",
    "serve": "^11.3.0",
    "source-map-explorer": "^2.2.2",
    "workbox-cli": "^5.0.0",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "resolutions": {
    "react-dev-utils": "^10.2.0"
  }
}
